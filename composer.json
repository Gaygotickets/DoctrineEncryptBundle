    "autoload": {
        "psr-0": { "VMelnik\\DoctrineEncryptBundle": "" }
    },
    "target-dir": "VMelnik/DoctrineEncryptBundle",
    "authors": [
        {
            "name": "Victor Melnik",
            "email": "melnikvictorl@gmail.com"
        }
    ],
    "require-dev": {
